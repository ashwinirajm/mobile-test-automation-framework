<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Android Unit Suite" parallel="false">

    <parameter name="platformName" value="ANDROID"/>

    <test name="Android Unit Tests">
        <groups>
            <run>
                <include name="unit"/>
            </run>
        </groups>

        <classes>
            <class name="unit.pages.GarageHomePageNegativeTest"/>
            <class name="unit.pages.LoginPageNegativeTest"/>
        </classes>
    </test>

</suite>
